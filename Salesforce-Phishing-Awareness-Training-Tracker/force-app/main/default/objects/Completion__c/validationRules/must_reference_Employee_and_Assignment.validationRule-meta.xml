<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>must_reference_Employee_and_Assignment</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
  ISBLANK(Assignment__c),
  ISBLANK(Employee__c)
)</errorConditionFormula>
    <errorMessage>Completion must be linked to both an Employee and an Assignment.</errorMessage>
</ValidationRule>
